package multipublish.consts
{
	
	/**
	 * 
	 * ElementTypeConsts 定义了几种排版元素类型常量。
	 * 
	 */
	
	
	import cn.vision.core.NoInstance;
	
	
	public final class ElementTypeConsts extends NoInstance
	{
		
		/**
		 * 
		 * 广告，需指定文件夹。
		 * 
		 * @default 2
		 * 
		 */
		
		public static const ADVERTISE:uint = 2;
		
		/**
		 * 
		 * 布局，排版。
		 * 
		 * @default 0
		 * 
		 */
		
		public static const ARRANGE:uint = 0;
		
		
		/**
		 * 
		 * 办公文档模式。
		 * 
		 * @default 5
		 * 
		 */
		
		public static const OFFICE:uint = 5;
		
		
		/**
		 * 
		 * 普通播放模式，视频，图片，SWF混播。
		 * 
		 * @default 1
		 * 
		 */
		
		public static const GENERAL:uint = 1;
		
		
		/**
		 * 
		 * 弹出窗口模式。
		 * 
		 * @default 6
		 * 
		 */
		
		public static const POPWINDOW:uint = 6;
		
		
		/**
		 * 
		 * 文件夹缩略图浏览模式。
		 * 
		 * @default 4
		 * 
		 */
		
		public static const THUMBNAIL:uint = 4;
		
		
		/**
		 * 
		 * 时间轴模式。
		 * 
		 * @default 7
		 * 
		 */
		
		public static const TIMELINE:uint = 7;
		
		
		/**
		 * 
		 * 网址。
		 * 
		 * @default 3
		 * 
		 */
		
		public static const WEBSITE:uint = 3;
		
	}
}